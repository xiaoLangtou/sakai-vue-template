{
    "name": "sakai-vue",
    "version": "4.3.0",
    "scripts": {
        "dev:rsbuild": "rsbuild dev",
        "build:pro": "vue-tsc && rsbuild build --mode production",
        "build:pro:rsdoctor": "cross-env RSDOCTOR=true rsbuild build --mode production",
        "prettier": "prettier --write .",
        "lint": "oxlint . --fix -D correctness --ignore-path .oxlintignore  && eslint . --max-warnings=0",
        "lint:eslint": " eslint . --fix",
        "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
        "prepare": "husky install"
    },
    "dependencies": {
        "@primeuix/themes": "^1.2.1",
        "@primevue/forms": "^4.3.6",
        "@tanstack/vue-query": "^5.28.4",
        "@vueuse/core": "^13.5.0",
        "axios": "^1.6.7",
        "chart.js": "3.3.2",
        "date-fns": "^4.1.0",
        "dayjs": "^1.11.13",
        "lucide-vue-next": "^0.525.0",
        "md5": "^2.3.0",
        "nprogress": "^0.2.0",
        "pinia": "^2.1.7",
        "pinia-plugin-persistedstate": "^4.4.1",
        "preline": "^3.2.3",
        "primeicons": "^7.0.0",
        "primevue": "^4.3.6",
        "tailwindcss-primeui": "^0.6.1",
        "vue": "^3.4.34",
        "vue-router": "^4.4.0",
        "zod": "^4.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@commitlint/cli": "^17.8.1",
        "@commitlint/config-conventional": "^17.8.1",
        "@esbuild-kit/cjs-loader": "^2.4.4",
        "@eslint/js": "^9.24.0",
        "@primevue/auto-import-resolver": "^4.3.6",
        "@rsbuild/core": "^1.2.18",
        "@rsbuild/plugin-babel": "^1.0.4",
        "@rsbuild/plugin-sass": "^1.2.2",
        "@rsbuild/plugin-vue": "^1.0.7",
        "@rsbuild/plugin-vue-jsx": "^1.1.0",
        "@tsconfig/node22": "^22.0.0",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/md5": "^2.3.5",
        "@types/node": "^22.13.10",
        "@types/nprogress": "^0.2.3",
        "@types/vue3-json-viewer": "^2.2.0",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.0",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.21",
        "commitizen": "^4.3.1",
        "commitlint-config-cz": "^0.13.3",
        "cross-env": "^7.0.3",
        "cz-customizable": "^7.4.0",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-oxlint": "^0.15.14",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-vue": "~10.0.0",
        "globals": "^16.0.0",
        "husky": "^8.0.3",
        "husky-hook": "^1.3.0",
        "oxlint": "^0.15.14",
        "plop": "^4.0.1",
        "postcss": "^8.5.3",
        "prettier": "3.5.3",
        "rollup-plugin-visualizer": "^5.14.0",
        "sass": "^1.85.1",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.8.2",
        "typescript-eslint": "^8.26.1",
        "unplugin-auto-import": "^19.1.1",
        "unplugin-vue-components": "^28.4.1",
        "vue-tsc": "^2.2.8"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
        }
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        }
    }
